<p id="notice"><%= notice %></p>

<h1>Finance States</h1>

<table>
  <thead>
    <tr>
      <th>Period</th>
      <th>Hibiscus sync</th>
      <th>Balance</th>
      <th>Account</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @finance_states.each do |finance_state| %>
      <tr>
        <td><%= finance_state.period %></td>
        <td><%= finance_state.hibiscus_sync_id %></td>
        <td><%= finance_state.balance %></td>
        <td><%= finance_state.account %></td>
        <td><%= link_to 'Show', finance_state %></td>
        <td><%= link_to 'Edit', edit_finance_state_path(finance_state) %></td>
        <td><%= link_to 'Destroy', finance_state, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Finance State', new_finance_state_path %>
